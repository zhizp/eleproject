        function formatDate(date) {
			   var y = date.getFullYear();
			    var m = date.getMonth() + 1;
			    m = m < 10 ? '0' + m : m;
			    var d = date.getDate();
			    d = d < 10 ? ('0' + d) : d;
			    return y + '-' + m + '-' + d;//这里可以写格式
			    //输出：2017-10-9
			}
