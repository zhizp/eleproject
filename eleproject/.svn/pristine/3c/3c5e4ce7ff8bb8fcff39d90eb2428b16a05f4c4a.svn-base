			}else if(this.barrageprice!=null && !this.barrageprice.toString().equals("0.00")){
				approvalRatio=this.approvalamount.divide(this.barrageprice,4, BigDecimal.ROUND_HALF_UP).doubleValue();
			}else {
				approvalRatio=0.00;
			}
		}else {
			approvalRatio=0.00;
		}
		return approvalRatio*100;
	}
