	public Double getLappropriationratio() {
		 /*<c:choose>
	     	<c:when test="${approval.totalappropriation!=null&&(approval.settlreviewamount!=null && approval.settlreviewamount>0)}">
	     		<fmt:formatNumber type="number" value="${approval.totalappropriation/approval.settlreviewamount * 100}" pattern="######0.00" maxIntegerDigits="6" maxFractionDigits="2"/>
	     	</c:when>
	     	<c:when test="${approval.totalappropriation!=null && approval.contractprice!=null && approval.contractprice>0}">
	     		<fmt:formatNumber type="number" value="${approval.totalappropriation/approval.contractprice * 100}" pattern="######0.00" maxIntegerDigits="6" maxFractionDigits="2"/>
	     	</c:when>
	     	<c:when test="${approval.totalappropriation!=null &&  approval.barrageprice!=null && approval.barrageprice>0}">
	     		<fmt:formatNumber type="number" value="${approval.totalappropriation/approval.barrageprice * 100}" pattern="######0.00" maxIntegerDigits="6" maxFractionDigits="2"/>
	     	</c:when>
	     	<c:otherwise>0.00</c:otherwise>
	     </c:choose>*/
		if(this.totalappropriation!=null && !this.totalappropriation.toString().equals("0.00")) {
			if(this.settlreviewamount!=null && !this.settlreviewamount.toString().equals("0.00")) {
				lappropriationratio=this.totalappropriation.divide(this.settlreviewamount,4,BigDecimal.ROUND_HALF_UP).doubleValue();
			}else if(this.contractprice!=null && !this.contractprice.toString().equals("0.00")){
				lappropriationratio=this.totalappropriation.divide(this.contractprice,4, BigDecimal.ROUND_HALF_UP).doubleValue();
			}else if(this.barrageprice!=null && !this.barrageprice.toString().equals("0.00")){
				lappropriationratio=this.totalappropriation.divide(this.barrageprice,4, BigDecimal.ROUND_HALF_UP).doubleValue();
			}else {
				lappropriationratio=0.00;
			}
		}else {
			lappropriationratio=0.00;
		}
		return lappropriationratio*100;
	}
	public void setLappropriationratio(Double lappropriationratio) {
		this.lappropriationratio = lappropriationratio;
	}
	public Double getApprovalRatio() {
		/*<c:if test="${approval.approvalamount!=null&&(approval.settlreviewamount!=null && approval.settlreviewamount>0)}">
		<fmt:formatNumber type="number" value="${approval.approvalamount/approval.settlreviewamount * 100}" pattern="######0.00" maxIntegerDigits="6" maxFractionDigits="2"/>
		</c:if>
		<c:if test="${approval.approvalamount==null||approval.settlreviewamount==null || approval.settlreviewamount<0.01}">
		0.00</c:if>*/
		if(this.approvalamount!=null && !this.approvalamount.toString().equals("0.00")) {
			if(this.settlreviewamount!=null && !this.settlreviewamount.toString().equals("0.00")) {
				approvalRatio=this.approvalamount.divide(this.settlreviewamount,4,BigDecimal.ROUND_HALF_UP).doubleValue();
			}else if(this.contractprice!=null && !this.contractprice.toString().equals("0.00")){
